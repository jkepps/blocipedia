<h1>Wikis</h1>
<%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
<div class="col-md-8">
	<% @wikis.each do |wiki| %>
		<div class="media">
			<div class="media-body">
				<h3 class="media-heading">
					<%= link_to wiki.title, wiki %>
				</h3> 
			</div>
		</div>
	<% end %>
</div>
