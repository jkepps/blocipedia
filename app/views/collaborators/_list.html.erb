<h3>Collaborators</h3>
<ul class="list-unstyled">
	<% users.each do |user| %>
		<li>
			<%= link_to user.username.capitalize, user %> 
			<%= link_to [wiki, wiki.collaborators.where(user: user).first], controller: :collaborators, method: :delete do %>
				<i class="glyphicon glyphicon-trash"></i>
			<% end %>
		</li>
	<% end %>
</ul>