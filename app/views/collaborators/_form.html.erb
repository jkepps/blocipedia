<%= form_for [wiki, Collaborator.new] do |f| %>
	<div class="form-group">
		<%= f.label :user %>
		<%= f.collection_select :user, elligible_collaborators(wiki),:id,:username, include_blank: true %>
	</div>
	<div class="form-group">
		<%= f.submit "Add Collaborator", class: 'btn btn-success' %>
	</div>
<% end %>